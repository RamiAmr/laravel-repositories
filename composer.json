{
    "name": "dugajean/repositories",
    "description": "Laravel Repositories",
    "license": "MIT",
    "homepage": "http://dukagj.in",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "repository",
        "repositories",
        "eloquent",
        "database"
    ],
    "authors": [
        {
            "name": "Dukagjin Surdulli",
            "email": "me@dukagj.in"
        }
    ],
    "require": {
        "php": "^7.2|^7.3|^8.0",
        "illuminate/support": "^5.4|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.4|^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^5.4|^6.0|^7.0|^8.0",
        "illuminate/console": "^5.4|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "@stable",
        "phpunit/phpunit": "^8.3|^8.5.19|^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Dugajean\\Repositories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dugajean\\Repositories\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dugajean\\Repositories\\Providers\\RepositoryProvider"
            ]
        }
    }
}
